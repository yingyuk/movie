extends ../layout

block content
	.container
		.row
			table.table.table-hover.table-bordered
				thead
					tr
						th 名字
						th 录入时间
						th 删除
				tbody
					each item in users
						tr(class="item-id-#{item._id}")
							td #{item.name}
							td #{moment(item.meta.updateAt).format('MM/DD/YYYY')}
							td
								button.btn.btn-danger.del.disabled(type="button",data-id="#{item._id}") 删除
	//- script(src="/js/user.js")
